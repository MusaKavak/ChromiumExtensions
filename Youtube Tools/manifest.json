{
    "manifest_version": 3,
    "name": "Youtube Tools",
    "description": "This extension adds some tools to Youtube.",
    "version": "1.0",
    "content_scripts": [
        {
            "js": [
                "./index.js"
            ],
            "matches": [
                "https://www.youtube.com/*"
            ]
        }
    ]
}